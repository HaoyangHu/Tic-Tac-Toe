{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "typecheck": "tsc",
    "format:check": "npx prettier --check \"**/*.{js,jsx,json,ts,tsx}\"",
    "format": "npx prettier --write \"**/*.{js,jsx,json,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run typecheck && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint",
    "**/*.{js,jsx,json,ts,tsx}": "prettier --write"
  },
  "dependencies": {
    "@expo-google-fonts/delius-unicase": "^0.2.2",
    "@react-native-async-storage/async-storage": "~1.14.1",
    "@react-native-community/netinfo": "^9.3.0",
    "@react-native-picker/picker": "^2.4.2",
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/native-stack": "^6.7.0",
    "@twotalltotems/react-native-otp-input": "^1.3.7",
    "amazon-cognito-identity-js": "^5.2.10",
    "aws-amplify": "^3.3.22",
    "aws-amplify-react-native": "^4.3.1",
    "expo": "~45.0.0",
    "expo-app-loading": "~2.0.0",
    "expo-av": "~11.2.3",
    "expo-font": "~10.1.0",
    "expo-haptics": "~11.2.0",
    "expo-linear-gradient": "~11.3.0",
    "expo-splash-screen": "~0.15.1",
    "expo-status-bar": "~1.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.1",
    "react-native-reanimated": "^2.9.1",
    "react-native-safe-area-context": "4.2.4",
    "react-native-screens": "~3.11.1",
    "react-native-web": "0.17.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.66.13",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^4.3.8",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "typescript": "~4.3.5"
  },
  "private": true
}
